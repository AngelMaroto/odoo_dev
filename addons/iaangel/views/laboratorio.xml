<odoo>
<data>
        <record id ="view_laboratorio_tree" model="ir.ui.view">
            <field name="name">iaangel.laboratorio.tree</field>
            <field name="model">iaangel.laboratorio</field>
            <field name="arch" type="xml">
                <tree string="Laboratorio">
                    <field name="nombre"/>
                    <field name="pais"/>
                    <field name="tipo"/>
                </tree>
            </field>
        </record>

        <record id="view_laboratorio_form" model="ir.ui.view">
            <field name="name">iaangel.laboratorio.form</field>
            <field name="model">iaangel.laboratorio</field>
            <field name="arch" type="xml">
                <form string="Laboratorio Form">
                    <sheet>
                        <group>
                            <field name="nombre"/>
                            <field name="pais"/>
                            <field name="tipo"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_iaangel_laboratorio" model="ir.actions.act_window">
            <field name="name">Laboratorio</field>
            <field name="res_model">iaangel.laboratorio</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="menu_iaangel_laboratorio" parent="menu_iaangel_root" action="action_iaangel_laboratorio"/>

</data>
</odoo>